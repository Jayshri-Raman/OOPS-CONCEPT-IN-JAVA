import java.util.*;

class Person
{
    int id;
    String name;
    Person(int id,String name)
    {
        this.id=id;
        this.name=name;
    }
}
class Emp extends Person
{
    double salary;
    
    Emp(int id,String name,double salary)
    {
        super(id,name);
        this.salary=salary;
        
    }

void display(){
    System.out.print(id+" "+name+" "+salary);
}
}
class Main
{
     public static void main (String[] args) {
        Emp e1=new Emp(14,"jai",5680.25);
        e1.display();
    }
}
